{
    "name": "Sketch Play",
    "description": "Preview one or more artboards in iPhone or iPad",
    "author": "@liguangming",
    "icon": "icon.png",
    "homepage": "http://liguangming.com/SketchPlay",
    "version": "1.0.2",
    "identifier": "com.liguangming.sketch-play",
    "manifestURL": "https://raw.githubusercontent.com/cute/SketchPlay/master/SketchPlay.sketchplugin/Contents/Sketch/manifest.json",
    "downloadURL": "https://github.com/cute/SketchPlay",
    "compatibleVersion": 3,
    "bundleVersion": 2,
    "commands": [{
        "name": "About",
        "identifier": "about-command",
        "shortcut": "",
        "script": "app.js",
        "handler": "onRun"
    }, {
        "name": "Show Toolbar",
        "identifier": "show-toolbar-command",
        "shortcut": "",
        "script": "app.js",
        "handler": "onRun",
        "handlers": {
            "actions": {
                "OpenDocument": "onRun"
            }
        }
    }, {
        "name": "Check for Updates...",
        "identifier": "check-update-command",
        "shortcut": "",
        "script": "app.js"
    }],
    "menu": {
        "isRoot": false,
        "items": [
            "about-command",
            "check-update-command"
        ]
    }
}
